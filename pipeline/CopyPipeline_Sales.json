								"source": {
									"type": "String",
									"ordinal": 7
								},
								"sink": {
									"name": "OrderQuantity",
									"type": "Int32",
									"physicalType": "int"
								}
							},
							{
								"source": {
									"type": "String",
									"ordinal": 8
								},
