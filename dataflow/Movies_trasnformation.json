{
	"name": "Movies_trasnformation",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Moviesdbff",
						"type": "DatasetReference"
					},
					"name": "MoviesADLS"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "filter1"
				},
				{
					"name": "DerivedPrimaryGenre"
				},
				{
					"name": "RankMoviesByRatings"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Column_1 as string,",
				"          Column_2 as string,",
				"          Column_3 as string,",
				"          Column_4 as string,",
				"          Column_5 as string,",
				"          Column_6 as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> MoviesADLS",
				"MoviesADLS select(mapColumn(",
				"          movie = Column_1,",
				"          title = Column_2,",
				"          genres = Column_3,",
				"          year = Column_4,",
				"          Ratings = Column_5,",
				"          {Rotten Tomato} = Column_6",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 filter(toInteger(year) > 1950) ~> filter1",
				"filter1 derive(column1 = iif(locate('|',genres) > 1,left(genres,locate('|',genres)-1),genres)) ~> DerivedPrimaryGenre",
				"DerivedPrimaryGenre window() ~> RankMoviesByRatings"
			]
		}
	}
}